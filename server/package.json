{
  "name": "@gobarber/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsnd --inspect --respawn --transpile-only src/server.ts",
    "start": "node dist/server",
    "typeorm": "tsnd ./node_modules/typeorm/cli.js",
    "migration": "npm run typeorm -- migration:run",
    "add-migration": "npm run typeorm -- migration:generate -n",
    "types:locales:generate": "i18next-typescript generate",
    "types:locales:watch": "i18next-typescript watch"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.2",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "i18next": "^19.8.4",
    "i18next-http-middleware": "^3.1.0",
    "i18next-node-fs-backend": "^2.1.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.30",
    "uuidv4": "^6.2.6",
    "envalid": "6.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/i18next-node-fs-backend": "^2.1.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.22",
    "i18next-typescript": "^0.1.0",
    "nodemon": "^2.0.7",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}

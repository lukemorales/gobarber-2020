{
  "name": "@gobarber/server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsnd -r tsconfig-paths/register --inspect --respawn --transpile-only src/shared/infra/http/server.ts",
    "start": "node dist/server",
    "typeorm": "tsnd -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:create": "npm run typeorm -- migration:generate -n",
    "types:locales:generate": "i18next-typescript generate",
    "types:locales:watch": "i18next-typescript watch",
    "test": "jest --passWithNoTests",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.2",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "envalid": "6.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "http-status-codes": "^2.1.4",
    "i18next": "^19.8.4",
    "i18next-http-middleware": "^3.1.0",
    "i18next-node-fs-backend": "^2.1.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.17",
    "pg": "^8.5.1",
    "pug": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.4.0",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/i18next-node-fs-backend": "^2.1.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.5",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.14.22",
    "@types/nodemailer": "^6.4.0",
    "@types/pug": "^2.0.4",
    "i18next-typescript": "^0.1.0",
    "jest": "^26.6.3",
    "nanoid": "^3.1.20",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  }
}
